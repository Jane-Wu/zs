<?php

/**
 * @file
 * Allows users to create and organize company information.
 */

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_FORM_ID_alter().
 */
function capital_company_form_node_company_form_alter(&$form, FormStateInterface $form_state) {
  $form['field_website']['widget'][0]['uri']['#value'] = !empty($form['field_website']['#default_value']) ? $form['field_website']['#default_value'] : 'http://';
}
